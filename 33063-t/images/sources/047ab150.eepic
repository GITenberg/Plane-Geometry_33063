\put( 4.2   , 15.2 ){$\scriptstyle B$}   %B
\put(15.2   ,  7.6 ){$\scriptstyle C$}   %C
\put( 4.2   ,  0.2 ){$\scriptstyle B'$}   %B'
\end{picture}
\PGrestore
