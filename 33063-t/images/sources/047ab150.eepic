\PGset[0.8em]
\begin{picture}   (16,16 )
\drawline     ( 1  , 8 )( 4.5,15 )(15, 8 )( 1, 8 )    %ABCA
\dashline[80]{0.2}( 1  , 8 )( 4.5, 1 )(15, 8)             %AB'C
\dashline[80]{0.2}( 4.5, 1 )( 4.5,15 )                    %B'B
